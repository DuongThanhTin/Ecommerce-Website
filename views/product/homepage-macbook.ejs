<section class="bg0 p-t-23 p-b-50">
    <div class="container">
        <div class="block1-txt-child1 flex-col-l">
            <span class="block1-name ltext-102 trans-04 p-b-8 translate">
                Macbook
            </span>
        </div>
        <div class="row isotope-grid">

            <% listmacbooks.forEach(function (product) { %>

            <div class="col-sm-6 col-md-4 col-lg-3 p-b-35 isotope-item women">
                <div class="block2">
                    <div class="block2-pic hov-img0">
                        <img src="<%= product.imagePath %>" alt="IMG-PRODUCT">

                        <a href="#" onclick="showdata('<%= product.productname %>', '<%= product.price %>','<%= product.description %>',
                                    '<%= product.imagePath %>','<%= product.category %>', '<%= product._id %>')"
                            class="block2-btn flex-c-m stext-103 cl2 size-102 bg0 bor2 hov-btn1 p-lr-15 trans-04 js-show-modal1">
                            Quick View
                        </a>
                    </div>

                    <div class="block2-txt flex-w flex-t p-t-14">
                        <div class="block2-txt-child1 flex-col-l ">
                            <a href="/products/<%= product._id %>"
                                class="stext-104 cl4 hov-cl1 trans-04 js-name-b2 p-b-6">
                                <%= product.productname %>
                            </a>
                            <a href="/products/<%= product._id %>"
                                class="stext-104 cl4 hov-cl1 trans-04 js-name-b2 p-b-6">
                                <%= product.description %>
                            </a>

                            <span class="stext-105 cl3">
                                $<%=product.price %>
                            </span>
                            <div>
                                <img src="/images/star.svg" alt="/" />
                                <img src="/images/star.svg" alt="/" />
                                <img src="/images/star.svg" alt="/" />
                                <img src="/images/star.svg" alt="/" />
                                <img src="/images/star.svg" alt="/" />
                            </div>
                        </div>

                        <div class="block2-txt-child2 flex-r p-t-3">

                        </div>
                    </div>
                </div>
            </div>

            <% }) %>

            <!-- Load more -->
            <!-- <div class="flex-c-m flex-w w-full p-t-45">
            <a href="#" class="flex-c-m stext-101 cl5 size-103 bg2 bor1 hov-btn1 p-lr-15 trans-04 translate">
                Load More
            </a>
        </div> -->
        </div>

</section>